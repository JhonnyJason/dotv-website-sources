mixin developer(name, nick, subtitle, imgurl)
    .d-block
        div
            h2 #{name} aka #{nick}
            h3=subtitle
        img(class='round')
      
    block

mixin ghrepo(slug, info)
  .d-block
    svg(): use(href="#svg-github-icon")
    div
      h1=slug
      if block
        block
      else
        h2=info
